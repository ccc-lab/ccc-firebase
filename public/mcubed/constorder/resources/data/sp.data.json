    "trial_instructions": {
      "stimulus": "<p class=\"text-center huge\">Califica la naturalidad de la oración que acabas de escuchar. Marca tu respuesta usando el teclado.</p>",
      "prompt": "<br/><br/><br/><p class=\"text-center huge\"><table class=\"huge\" style=\"text-align: center;\"><tr><td>1</td><td>2</td><td>3</td><td>4</td><td>5</td><td>6</td><td>7</td></tr><tr><td>muy antinatural</td><td></td><td></td><td>ni natural ni antinatural</td><td></td><td></td><td>muy natural</td></tr></table></p>"
